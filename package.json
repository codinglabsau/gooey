{
  "name": "@codinglabsau/gooey",
  "version": "2.0.1",
  "type": "module",
  "files": [
    "dist",
    "src"
  ],
  "main": "./dist/gooey.umd.cjs",
  "module": "./dist/gooey.js",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/gooey.js",
      "require": "./dist/gooey.umd.cjs"
    },
    "./presets/v3": {
      "types": "./src/presets/v3/index.ts",
      "import": "./src/presets/v3/index.ts",
      "require": "./src/presets/v3/index.ts"
    },
    "./presets/v4": "./dist/presets/v4/preset.css",
    "./presets/slate.css": "./dist/presets/slate.css",
    "./style.css": "./dist/gooey.css",
    "./dist/presets/*.css": {
      "import": "./dist/presets/*.css",
      "require": "./dist/presets/*.css"
    }
  },
  "types": "./dist/types/index.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "run-p format && vite build && vue-tsc --declaration --emitDeclarationOnly",
    "prepare": "relative-deps",
    "type-check": "vue-tsc --declaration --emitDeclarationOnly",
    "format": "run-p prettier lint",
    "prettier": "prettier 'src/**/*.{js,ts,vue}' 'app/**/*.{js,ts,vue}' --write",
    "lint": "eslint 'src/**/*.{js,ts,vue}' 'app/**/*.{js,ts,vue}' --fix",
    "test": "cypress run"
  },
  "publishConfig": {
    "@codinglabsau:registry": "https://npm.pkg.github.com"
  },
  "peerDependencies": {
    "tailwindcss": "^3.3.0 || ^4.0.0",
    "tailwindcss-animate": "^1.0.0",
    "tw-animate-css": "^1.0.0",
    "vue": "^3.2.0"
  },
  "peerDependenciesMeta": {
    "tailwindcss": {
      "optional": true
    },
    "tailwindcss-animate": {
      "optional": true
    },
    "tw-animate-css": {
      "optional": true
    }
  },
  "dependencies": {
    "@tanstack/vue-table": "^8.21.2",
    "@unovis/ts": "^1.6.2",
    "@unovis/vue": "^1.6.2",
    "@vee-validate/zod": "^4.15.1",
    "@vueuse/core": "^14.1.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "embla-carousel-vue": "^8.3.0",
    "lucide-vue-next": "^0.562.0",
    "reka-ui": "^2.7.0",
    "tailwind-merge": "^3.4.0",
    "vaul-vue": "^0.4.1",
    "vee-validate": "^4.15.1",
    "vue-input-otp": "^0.3.2",
    "vue-sonner": "^2.0.9",
    "zod": "^3.25.76"
  },
  "overrides": {
    "@types/d3-dispatch": "3.0.6"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@tailwindcss/typography": "^0.5.15",
    "@tailwindcss/vite": "^4.1.18",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vue/tsconfig": "^0.8.1",
    "cypress": "^15.9.0",
    "cypress-vite": "^1.5.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-vue": "^10.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.8.0",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "relative-deps": "^1.0.7",
    "tailwindcss": "^4.1.18",
    "tailwindcss-animate": "^1.0.7",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.53.0",
    "unplugin-vue-markdown": "^29.2.0",
    "vite": "^7.3.1",
    "vite-plugin-static-copy": "^3.1.4",
    "vue": "^3.2.37",
    "vue-router": "^4.4.4",
    "vue-tsc": "^3.2.2"
  }
}
