{
  "name": "@codinglabsau/gooey",
  "version": "1.0.0-alpha.1",
  "type": "module",
  "files": [
    "dist",
    "src"
  ],
  "main": "./dist/gooey.umd.cjs",
  "module": "./dist/gooey.js",
  "exports": {
    ".": {
      "import": "./dist/gooey.js",
      "require": "./dist/gooey.umd.cjs"
    },
    "./dist/presets/*.css": {
      "import": "./dist/presets/*.css",
      "require": "./dist/presets/*.css"
    }
  },
  "types": "./dist/types/index.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "run-p format type-check && vite build",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "prepare": "relative-deps",
    "type-check": "vue-tsc --declaration --emitDeclarationOnly",
    "format": "run-p prettier lint",
    "prettier": "prettier 'src/**/*.{js,ts,vue}' 'app/**/*.{js,ts,vue}' --write",
    "lint": "eslint 'src/**/*.{js,ts,vue}' 'app/**/*.{js,ts,vue}' --fix",
    "test": "cypress run",
    "testc": "cypress run --component",
    "teste": "cypress run --e2e"
  },
  "publishConfig": {
    "@codinglabsau:registry": "https://npm.pkg.github.com"
  },
  "peerDependencies": {
    "@inertiajs/vue3": "^1.0.0",
    "vue": "^3.2.0"
  },
  "dependencies": {
    "@heroicons/vue": "^2.0.0",
    "@radix-icons/vue": "^1.0.0",
    "@vueuse/core": "^9.11.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "lucide-vue-next": "^0.439.0",
    "radix-vue": "^1.9.5",
    "tailwind-merge": "^2.5.2",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@inertiajs/vue3": "^1.0.0",
    "@tailwindcss/forms": "^0.5.2",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "@vitejs/plugin-vue": "^5.0.0",
    "@vue/tsconfig": "^0.1.3",
    "autoprefixer": "^10.4.7",
    "cypress": "^13.13.3",
    "cypress-vite": "^1.5.0",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-tailwindcss": "^3.10.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "eslint-plugin-vue": "^9.9.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.14",
    "prettier": "^2.8.7",
    "relative-deps": "^1.0.7",
    "tailwindcss": "^3.3.0",
    "typescript": "^4.9.5",
    "vite": "^5.0.0",
    "vite-plugin-static-copy": "^1.0.6",
    "vitepress": "^1.0.0",
    "vue": "^3.2.37",
    "vue-router": "^4.4.4",
    "vue-tsc": "^1.2.0"
  }
}
